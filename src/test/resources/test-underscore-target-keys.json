[
  {
    "maskingConfig": {
      "maskKeys": [
        "someKey"
      ]
    },
    "input": {
      "someKey": "hello",
      "some_key": "hello",
      "some_Key": "hello",
      "otherKey": null
    },
    "expectedOutput": {
      "someKey": "***",
      "some_key": "***",
      "some_Key": "***",
      "otherKey": null
    }
  },
  {
    "maskingConfig": {
      "maskKeys": [
        "Key"
      ]
    },
    "input": {
      "_key": "hello",
      "otherKey": null
    },
    "expectedOutput": {
      "_key": "hello",
      "otherKey": null
    }
  },
  {
    "maskingConfig": {
      "maskKeys": [
        "some_Key"
      ]
    },
    "input": {
      "some_key": "hello",
      "some_Key": "hello",
      "some__key": "not this one"
    },
    "expectedOutput": {
      "some_key": "***",
      "some_Key": "***",
      "some__key": "not this one"
    }
  },
  {
    "maskingConfig": {
      "maskKeys": [
        "someKey"
      ],
      "caseSensitiveTargetKeys": true
    },
    "input": {
      "someKey": "hello",
      "some_key": "hello",
      "some_Key": "hello",
      "otherKey": null
    },
    "expectedOutput": {
      "someKey": "***",
      "some_key": "***",
      "some_Key": "hello",
      "otherKey": null
    }
  },
  {
    "maskingConfig": {
      "allowKeys": [
        "allowedKey"
      ]
    },
    "input": {
      "allowedKey": "hello",
      "allowed_key": "hello",
      "allowed_Key": "hello",
      "otherKey": "masked"
    },
    "expectedOutput": {
      "allowedKey": "hello",
      "allowed_key": "hello",
      "allowed_Key": "hello",
      "otherKey": "***"
    }
  },
  {
    "maskingConfig": {
      "maskKeys": [
        "someKey",
        "someOtherKey"
      ]
    },
    "input": {
      "some_key": "hello",
      "some_other_key": "hello"
    },
    "expectedOutput": {
      "some_key": "***",
      "some_other_key": "***"
    }
  },
  {
    "maskingConfig": {
      "allowKeys": [
        "allowedKey"
      ],
      "caseSensitiveTargetKeys": true
    },
    "input": {
      "allowedKey": "hello",
      "allowed_key": "hello",
      "allowed_Key": "masked",
      "otherKey": "masked"
    },
    "expectedOutput": {
      "allowedKey": "hello",
      "allowed_key": "hello",
      "allowed_Key": "***",
      "otherKey": "***"
    }
  }
